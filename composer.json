{
    "name": "somnambulist/read-models",
    "type": "library",
    "description": "A stripped down active-record'ish query layer for fetching data from your database.",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "ext-json":  "*",
        "doctrine/dbal": "~2.9",
        "doctrine/inflector": "~1.3",
        "pagerfanta/pagerfanta": "~2.2",
        "pragmarx/ia-str": "^6.0|^7.0",
        "somnambulist/collection": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.2",
        "creof/doctrine2-spatial": "~1.2",
        "doctrine/doctrine-bundle": "~2.0",
        "doctrine/orm": "^2.6",
        "fzaninotto/faker": "~1.8",
        "somnambulist/domain": "~2.0",
        "symfony/dotenv": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/messenger": "^5.0",
        "symfony/stopwatch": "^5.0",
        "symfony/var-dumper": "^5.0",
        "victorjonsson/markdowndocs": "^1.3",
        "symfony/yaml": "^5.0",
        "vimeo/psalm": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\ReadModels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\ReadModels\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "doctrine/orm": "Use full weight Doctrine for managing persistence and data",
        "doctrine/migrations": "To build schema changes",
        "symfony/http-kernel": "To use the kernel listener, requires >=5.0"
    }
}
